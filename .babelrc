{
  presets: [
    'es2015',
    'es2016',
    'react'
  ],
  plugins: [
    'transform-class-properties',
    'transform-object-rest-spread',
    ['transform-regenerator', {async: false}],
    ['transform-runtime', {
      helpers: false,
      polyfill: false,
      regenerator: true,
    }]
  ]
}
